@model User
@{
    ViewData["Title"] = "Home Page";
    int? userId = Context.Session.GetInt32("userId");
}


<!DOCTYPE html>
<html>

@if (userId == null)
{
    @await Html.PartialAsync("_VisitorNav")

}
else
{
    if (Model.Role.ToString() == "Shipper")
    {
        @await Html.PartialAsync("_ShipperNav")
    }
    else if (Model.Role.ToString() == "User")
    {
        @await Html.PartialAsync("_UserNav")
    }
    else
    {
        @await Html.PartialAsync("_AdminNavBar")
    }
}


<head>
    <meta charset="utf-8" />
    <title>Your Page Title</title>
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet">
    <link rel="stylesheet" href="~/css/CardHome1.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Event.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Counter.css" asp-append-version="true" />

</head>
<body>
    <div>
        <div style="background: linear-gradient(rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0)), url('https://scontent.ftun10-1.fna.fbcdn.net/v/t1.6435-9/119176965_3550247041675870_2428150126869658769_n.jpg?_nc_cat=105&ccb=1-7&_nc_sid=7f8c78&_nc_ohc=pnKVH6ytfVgAX9k14vq&_nc_ht=scontent.ftun10-1.fna&oh=00_AfAsNPbGKEGDoHsB4x_AOKY2WWbuhFjO92Rpx36ZXK8WwQ&oe=657959C4') no-repeat fixed; height: 100vh; background-size:cover; padding: 100px 0px 100px 0px; clip-path: polygon(100% 0%, 100% 95%, 24% 100%, 0 95%, 0 0, 25% 0);">
            <div class="col-md-12 " style="padding:10rem">
                <h1 style="font-size: 3rem; color: red; ">Red Crescent</h1>
                <hr>
                <h2 style="color:white; font-size: 2rem;">
                    Despite the danger, we work in more than 244 delegations.<br />
                    Faithful to the principles of humanity,<br />
                    neutrality, impartiality and independence.
                </h2>
            </div>
        </div>
    </div>

    <h1 style="margin-top:50px; text-align:center ;font-family:  Roboto,Arial,Helvetica Neue,Helvetica,sans-serif;
        font-size: 3.2rem;
        font-weight: 900;
        line-height: 1.2;
        color: #333;">
        You Can Make a Difference
    </h1>

    <div class="card-container">
        <section class="hero-section">
            <div class="card-grid  card-container">
                <a class="card" href="#">
                    <div class="card__background" style="background-image: url(https://scontent.ftun10-1.fna.fbcdn.net/v/t1.6435-9/119277947_3550245648342676_1132457996825520403_n.jpg?_nc_cat=101&ccb=1-7&_nc_sid=7f8c78&_nc_ohc=IeJRK7q0-GQAX9V8hFO&_nc_ht=scontent.ftun10-1.fna&oh=00_AfBp-AljEDIESWE5lp9wFDlAlNQAtyaqbxj0ijqCPOT54w&oe=657969E7) "></div>
                    <div class="card__content">
                        <h3 class="card__heading">Volunteer</h3>
                    </div>
                </a>
                <a class="card" href="#">
                    <div class="card__background" style="background-image: url(https://www.tunisienumerique.com/wp-content/uploads/2023/10/CRT.jpg)"></div>
                    <div class="card__content">
                        <h3 class="card__heading">Donor</h3>
                    </div>
                </a>
            </div>
        </section>
    </div>
    <br />
    <h1 style="margin-top:50px; text-align:center;font-family: Roboto,Arial,Helvetica Neue,Helvetica,sans-serif;
        font-size: 3.2rem;
        font-weight: 900;
        line-height: 1.2;
        color: #333;">
        Join Our Community Of Donors
    </h1>


    <div class="container mt-5" style=" background: linear-gradient(to right, #c4181c, #ffffff); ">
        <div class="wrapper">
            <div class="counter col_fourth">
                <h2 class="timer count-title count-number" data-to="300" data-speed="3000" style=" font-size: 4rem;"></h2>
                <p class="count-text ">Volunteers on Site</p>
            </div>

            <div class="counter col_fourth">
                <h2 class="timer count-title count-number" data-to="1700" data-speed="3000" style=" font-size: 4rem;"></h2>
                <p class="count-text ">Donates</p>
            </div>

            <div class="counter col_fourth">
                <h2 class="timer count-title count-number" data-to="11900" data-speed="3000" style=" font-size: 4rem;"></h2>
                <p class="count-text ">All Members</p>
            </div>

            <div class="counter col_fourth end">
                <h2 class="timer count-title count-number" data-to="157" data-speed="3000" style=" font-size: 4rem;"></h2>
                <p class="count-text ">Center</p>
            </div>
        </div>

    </div>

    <div class="container" style="display:flex;justify-content:space-between;margin-top:20rem">
        <div class="col-md-5 col-lg-5">
            <h2 class="title-uppercase"> Non-profit organization </h2>
            <span style="color: red ; font-weight:bold;"> The Tunisian Red Crescent </span> is a public interest organization, created on October 7, 1956, and recognized by decree of May 6, 1957 as a voluntary aid and relief organization auxiliary to public authorities in the humanitarian field, in accordance with the Geneva Conventions of 1949 and its additional protocols.<span style="color: red; font-weight:bold;"> Le Croissant Rouge Tunisien </span>
            The Tunisian Red Crescent became a member of the International Red Cross and Red Crescent movement on September 13, 1957.5
        </div>

        <div class="col-md-6 col-lg-offset-1">
            <div class="progress-bars">
                <div style="color: #4B4B4C; font-weight: bold; display:flex; justify-content:space-between">
                    <div class="number pull-left">MONEY</div>
                    <div class="number pull-right">21</div>
                </div>
                <div class="progress">
                    <div class="progress-bar" role="progressbar" style="width: 60%" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
                <div style="color: #4B4B4C; font-weight: bold; display:flex; justify-content:space-between; margin-top:2rem">
                    <div class="number pull-left">MEDICATION</div>
                    <div class="number pull-right">244</div>
                </div>
                <div class="progress">
                    <div class="progress-bar" role="progressbar" style="width: 45%" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
                <div style="color: #4B4B4C; font-weight: bold; display: flex; justify-content: space-between; margin-top: 2rem">
                    <div class="number pull-left">CLOTHING</div>
                    <div class="number pull-right">2500</div>
                </div>

                <div class="progress">
                    <div class="progress-bar" role="progressbar" style="width: 80%" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
                <div style="color: #4B4B4C; font-weight: bold; display: flex; justify-content: space-between; margin-top: 2rem">
                    <div class="number pull-left">FOOD</div>
                    <div class="number pull-right">1824</div>
                </div>

                <div class="progress">
                    <div class="progress-bar" role="progressbar" style="width: 76%" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
                <div style="color: #4B4B4C; font-weight: bold; display: flex; justify-content: space-between; margin-top: 2rem">
                    <div class="number pull-left">EQUIPMENT</div>
                    <div class="number pull-right">412</div>
                </div>

                <div class="progress">
                    <div class="progress-bar" role="progressbar" style="width: 30%" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
            </div>
        </div>
    </div>

    <h1 style="margin-top:100px ;    font-family: Roboto,Arial,Helvetica Neue,Helvetica,sans-serif;
        font-size: 3.2rem;
        font-weight: 900;
        line-height: 1.2;
        color: #333; text-align:center">
        OUR COMMUNITY
    </h1>

    <div class="d-flex justify-content-center mt-5">
        <article class="gallery">
            <img src="data:image/jpeg;base64," alt="Red Crescent1" />
            <blockquote class="quote spacer">
                <span>There is not just one day when we need to offer our help to people around us but we have that duty on us to do it every day</span>
            </blockquote>
            <img src="https://scontent.ftun10-1.fna.fbcdn.net/v/t1.6435-9/119197588_3550247338342507_7954835345146434608_n.jpg?_nc_cat=104&ccb=1-7&_nc_sid=7f8c78&_nc_ohc=amz_RSj9P_kAX_y2U0x&_nc_ht=scontent.ftun10-1.fna&oh=00_AfDPEZuKjbrrS4qSOUzK94UCzhF9932nwCwqpqsBE9YeEg&oe=65797F61" alt="Red Crescent2" />
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQgnkghCa7H3YaEEM8c_9jfrlQPqEbnTdPfWQ&usqp=CAU" alt="Red Crescent3" />
            <img src="data:image/jpeg;base64," alt="Red Crescent4" />
            <img src="https://ar.lemaghreb.tn/media/k2/items/cache/40f4e3a5a1a71fd73bc225876252217c_XL.jpg" alt="Red Crescent5" />
            <div class="spacer"></div>
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRYilv_KVP11b6vn1pnVbiCvDuHIdRbNnygqVvN3mI2_Hh8eV-l5LnF-ghd1Io0tF92xVg&usqp=CAU" alt="Red Crescent6" />
            <img src="https://cdnuploads.aa.com.tr/uploads/Contents/2023/07/23/thumbs_b_c_223423c4c73002a500b8e3c491c89406.jpg?v=212933" alt="Red Crescent7" />
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRDozFd5wqmhPWuiFEil6ujgoAHNycedGzLvg&usqp=CAU" alt="Red Crescent8" />
            <div class="spacer"></div>
            <div class="spacer"></div>
            <img src="https://img.freepik.com/premium-vector/tunisia-flag-silk-waving-flag-tunisia-tunisia-flag-pole-independence-day_650465-319.jpg" alt="Red Crescent9" />
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTfUe4iYric7i4Myg_J1H3a697VeV1puQj-QA&usqp=CAU" alt="Red Crescent10" />
            <img src="https://www.businessnews.com.tn/images/album/IMGBN101309aides.jpg" alt="Red Crescent11" />
            <div class="spacer"></div>
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSCk_tDtQd4Z5q7nhulbUWsRtpVqEvWZuEd6kk9ivrS48-9-B92NNVH3lgM9RIoLnK_4-Y&usqp=CAU" alt="Red Crescent12" />
            <img src="https://carthagemagazine.com/wp-content/uploads/2023/10/Tunisian-Humanitarian-Aid-for-Gaza.jpg" alt="Red Crescent13" />
        </article>
    </div>



    <h1 style="margin-top: 150px; display: flex; justify-content:center; font-family:  Roboto,Arial,Helvetica Neue,Helvetica,sans-serif;
        font-size: 3.2rem;
        font-weight: 900;
        line-height: 1.2;
        color: #333; ">
        The Fundamental Principles of the Red Crescent
    </h1>
    <div style="display:flex;justify-content:center  ; padding:70px ;margin-left:200px ">

        <div class="card2" style="width: 390px;  ">
            <img src="https://www.croissant-rouge.tn/img/reviews/humanite.jpg" alt="..." style="height:100px">
            <div class="card2-body">
                <h1>HUMANITY </h1>

                <p class="card-text" style="width:250px">
                    Born from the desire to bring aid without discrimination to the injured in the fields of
                    battle, the International Red Cross and Red Crescent Movement,
                    strives to prevent and alleviate suffering in all circumstances
                    human.
                    It tends to protect life and health as well as to ensure respect for the person
                    human.
                    It promotes mutual understanding, friendship, cooperation and peace
                    sustainable
                    between all peoples.
                </p>
            </div>
        </div>

        <div class="card2" style="width: 390px;">
            <img src="https://www.croissant-rouge.tn/img/reviews/IMPARTIALITe.jpg" alt="..." style="height:100px">
            <div class="card2-body">
                <h1>IMPARTIALITY  </h1>
                <p class="card-text" style="width:250px">

                    It makes no distinction of nationality, race, religion, condition
                    social and political affiliation. It only applies to helping the
                    individuals to the extent of their suffering and to provide priority to those
                    most urgent distresses.

                </p>
            </div>
        </div>


        <div class="card2" style="width: 390px;">
            <img src="https://www.croissant-rouge.tn/img/reviews/NEUTRALITe.jpg" alt="..." style="height:100px">
            <div class="card2-body">
                <h1>NEUTRALITY  </h1>
                <p class="card-text" style="width:250px">
                    In order to maintain everyone's confidence, the movement refrains from taking part in
                    hostilities and, at all times, controversies of a political, racial,
                    religious and ideological.
                </p>
            </div>
        </div>
    </div>




</body>
</html>